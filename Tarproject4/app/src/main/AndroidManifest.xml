<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.armen.tarproject4">

    <application
        android:allowBackup="true"
        android:icon="@drawable/colors"
        android:label="@string/labelName"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ColorPoolActivity" />
        <activity android:name=".RelativeActivity" />
        <activity android:name=".FrameActivity" />
        <activity android:name=".ConstraintActivity">
        </activity>
        <activity
            android:name=".CoordinatorActivity"
            android:label="@string/title_activity_coordinator"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        </activity>
    </application>

</manifest>
